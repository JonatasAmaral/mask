{
  "name": "@tadashi/mask",
  "version": "1.6.0",
  "description": "The simple and tiny script for input mask",
  "keywords": [
    "mask",
    "input",
    "tiny"
  ],
  "bug": {
    "url": "https://github.com/lagden/mask",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "https://lagden.in"
  },
  "main": "dist/index.umd.js",
  "repository": "lagden/mask",
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava --serial",
    "rollup": "rollup -c",
    "prebuild": "npm test && rm -rf dist",
    "build": "npm run rollup",
    "start": "npm run build"
  },
  "files": [
    "dist",
    "src",
    "license",
    "README.md"
  ],
  "xo": {
    "envs": [
      "es6"
    ],
    "globals": [
      "document",
      "window",
      "HTMLElement",
      "HTMLInputElement"
    ],
    "esnext": true,
    "semicolon": false,
    "rules": {
      "capitalized-comments": 0,
      "unicorn/prevent-abbreviations": 0,
      "no-constructor-return": 0
    }
  },
  "esm": {
    "cjs": {
      "dedefault": true
    }
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/helpers/**/*"
    ],
    "require": [
      "esm",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "devDependencies": {
    "ava": "3.13.0",
    "esm": "3.2.25",
    "jsdom": "16.4.0",
    "rollup": "2.32.1",
    "simulant": "0.2.2",
    "xo": "0.34.1"
  },
  "dependencies": {
    "@tadashi/hex-id": "2.0.0"
  }
}
