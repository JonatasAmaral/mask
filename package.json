{
  "name": "@tadashi/mask",
  "version": "1.4.1",
  "description": "The simple and tiny script for input mask",
  "keywords": [
    "mask",
    "input",
    "tiny"
  ],
  "bug": {
    "url": "https://github.com/lagden/mask",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "https://lagden.in"
  },
  "main": "dist/index.js",
  "repository": "lagden/mask",
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava --serial",
    "rollup": "rollup -c",
    "prebuild": "npm test",
    "build": "npm run rollup",
    "start": "npm run build"
  },
  "files": [
    "dist",
    "src",
    "license",
    "README.md"
  ],
  "xo": {
    "envs": [
      "es6"
    ],
    "globals": [
      "document",
      "window",
      "HTMLElement",
      "HTMLInputElement"
    ],
    "esnext": true,
    "semicolon": false,
    "rules": {
      "capitalized-comments": 0
    }
  },
  "ava": {
    "babel": false,
    "extensions": [
      "js",
      "mjs"
    ],
    "helpers": [
      "test/helpers/**/*"
    ],
    "require": [
      "esm",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "devDependencies": {
    "ava": "2.4.0",
    "esm": "3.2.25",
    "jsdom": "15.2.0",
    "rollup": "1.25.0",
    "simulant": "0.2.2",
    "xo": "0.25.3"
  }
}
